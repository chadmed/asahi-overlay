<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE pkgmetadata SYSTEM "https://www.gentoo.org/dtd/metadata.dtd">
<pkgmetadata>
<maintainer type="project">
  <email>asahi@gentoo.org</email>
</maintainer>
<use>
  <flag name="bin-symlinks">
    Symlink executables to usr/bin to be in default
    PATH (conflicts with crossdev, and crossdev
    is preferable for a fully featured toolchain)
  </flag>
</use>
<upstream>
  <remote-id type="cpe">cpe:/a:gnu:gcc</remote-id>
  <remote-id type="cpe">cpe:/a:gnu:binutils</remote-id>
  <remote-id type="cpe">cpe:/a:gnu:glibc</remote-id>
  <remote-id type="cpe">cpe:/o:linux:linux_kernel</remote-id>
</upstream>
</pkgmetadata>
